<h1>Log in here to see whats listed! </h1>
<%= form_tag(login_path) do %>
    <%= text_field_tag :username, '', :placeholder => "Username" %>
    <%= password_field_tag :password, '', :placeholder => "Password" %>
    

    <%= submit_tag "Sign In" %>
<%end%>
<%= link_to('Log in with GitHub!', '/auth/github') %>


<footer><%= link_to("not a member? Sign up here!", new_user_path)%></footer>